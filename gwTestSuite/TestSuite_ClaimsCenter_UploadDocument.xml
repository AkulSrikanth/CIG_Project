<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" parallel="methods"
	name="ClaimsCenter TestSuite">

	<listeners>
		<listener
			class-name="com.Utils.Selenium.Selenium_TestNG_TestListener" />
	</listeners>

	<test thread-count="1" parallel="methods"
		name="ClaimsCenter Smoke Test">
		<classes>
			<class
				name="com.Test.Guidewire.ClaimsCenter.GW_CC_UploadDocument">
				<methods>
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_Txt" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_PDF" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_BitmapImage" />
					<include
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_MicrosoftExcelWorksheet" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_GIFImage" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_HTML" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_JPEGImage" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_MPEGVideo" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_OpenXMLspreadsheet" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_PNGImage" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_OpenXMLpresentation" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_QuickTimeVideo" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_TiffImage" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_XML" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_MicrosoftAudio" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_OCTETStream" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_WaveAudio" />
					<exclude
						name="AUT_PA_CC_FNOL_VerifiedPolicy_UploadDocument_WordDocument" />

				</methods>
			</class>
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
